<%- include('./layout/header.ejs', {doc_title:"Talent"}) %>
<%- include('./layout/nav.ejs') %>
<div class='container'>
    <h1 class='mt-5'><%= data.name %></h1>
    <p><%=data.schedule %></p>
    <table class='table mt-3'>
        <tr class='bg-info'>
            <th>No</th>
            <th>Name</th>
            <th>Age</th>
            <th>Gender</th>
            <th>Talent</th>
        </tr>
        <% data.Talents.forEach((data, index) =>{ %>
            <tr>
                <td><%= index+1 %></td>
                <td><%= data.name %></td>
                <td><%= data.age %></td>
                <td><%= data.gender %></td>
                <td><%= data.talent %></td>
            </tr>
        <%}) %>
        
    </table>
</div>
<%- include('./layout/footer.ejs') %>